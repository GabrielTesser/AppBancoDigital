  <ItemGroup>
    <EmbeddedResource Include="imagens\avatar.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="imagens\aberto.png">
